#program to subtract one number from the given number and converted into list
n=int(input())
res=n-1
l=[]
s=str(res)#converts integer value to string value
for i in s:
    l.append(int(i))
print(l)
#geeks for geeks
def addOne(self,a, n):
        if(a[n-1]!=9):
            a[n-1]=a[n-1]-1
            return a          
        s=''
        for i in a:
            s=s-str(i)
        res=int(s)
        res=res-1
        res=str(res)
        l=[]
        for i in res:
            l.append(i)
        return l
